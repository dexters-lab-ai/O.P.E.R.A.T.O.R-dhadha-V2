  "author": "Aident AI",
  "scripts": {
    "browserless:debugger": "open http://localhost:11976/debugger/?token=be19d9f2-aa21-46dc-945c-2880c371719e",
    "browserless:kill-all": "npm run web:run-script -- scripts/browserless/kill-all-sessions.ts",
    "browserless:package": "cd apps/browserless && npm run package",
    "browserless:sessions": "open http://localhost:11976/sessions?token=be19d9f2-aa21-46dc-945c-2880c371719e",
    "browserless:start": "cd apps/browserless && npm run start",
    "browserless:start-prod": "cd apps/browserless && npm run start-prod",
    "browserless:status": "cd apps/browserless && npm run status",
    "browserless:stop": "cd apps/browserless && npm run stop",
    "build": "npx turbo build",
    "clear-node-modules": "rm -rf node_modules && rm -rf apps/web/node_modules && rm -rf apps/extension/node_modules && rm -rf apps/docs/node_modules && rm -rf packages/shared/node_modules && rm -rf packages/ui/node_modules && rm -rf packages/eslint-config/node_modules && rm -rf packages/typescript-config/node_modules",
    "dev": "npm run envs:pull && npm run supabase:storage:init && npm run supabase:mock-user:init && npx turbo dev --concurrency 20",
    "docker:down": "docker-compose -f docker/docker-compose.build.yaml down && docker-compose -f installer/supabase-docker/docker-compose.yml down",
    "docker:prod:down": "bash quick-stop.sh",
    "docker:prod:reset": "bash quick-reset.sh",
    "docker:prod:up": "bash quick-start.sh",
    "docker:restart": "npm run docker:down && npm run docker:start",
    "docker:start": "npx turbo docker:start && bash quick-start.sh --build",
    "docker:tag:stable": "bash scripts/tag-stable-image-version.sh",
    "envs:pull": "sh ./scripts/pull-envs-for-all-packages.sh",
    "envs:remove": "npm run envs:rm",
    "envs:reset": "npm run envs:pull -- --reset",
    "envs:rm": "sh ./scripts/rm-envs-for-all-packages.sh",
    "format": "npx prettier . --write && npx turbo format",
    "generate-export-map": "npm run run-script -- ./scripts/generate-export-map.ts",
    "init:linux": "pnpm i && npm run envs:pull && sh scripts/install-gsed-on-linux.sh --classic && apt-get install docker-compose && npm run supabase:restart",
    "init:mac": "pnpm i && npm run envs:pull && brew install gsed docker-compose && npm run supabase:restart",
    "mac:build": "npm run mac:clean && npm run mac:build:app && npm run mac:build:wait && cd installer/mac-app && bash scripts/sign-mac-app.sh && bash scripts/build-mac-installer.sh",
    "mac:build:app": "cd installer/mac-app && open OpenCuak.platypus",
    "mac:build:wait": "cd installer/mac-app && bash scripts/wait-for-app.sh",
    "mac:clean": "rm -rf installer/mac-app/build && rm -rf installer/mac-app/OpenCuak.app",
    "mac:images": "cd installer/mac-app && bash scripts/download-docker-images.sh",
    "mac:notarize": "cd installer/mac-app && bash scripts/notarize-mac-artifact.sh",
    "mac:notarize:wait": "cd installer/mac-app && bash scripts/notarize-mac-artifact.sh --wait",
    "mac:notarize:check": "cd installer/mac-app && bash scripts/check-notarization-status.sh",
    "quick:reset": "bash quick-reset.sh",
    "quick:restart": "npm run quick:stop && npm run quick:start",
    "quick:start": "bash quick-start.sh",
    "quick:stop": "bash quick-stop.sh",
    "rebase": "git checkout . && git fetch && git rebase -i origin/main && npm run envs:pull",
    "repl": "ts-node --esm --interactive --compilerOptions '{\"target\": \"esnext\"}' --pretty",
    "run-script": "TS_NODE_PROJECT=./tsconfig.json ts-node -r tsconfig-paths/register",
    "start-prod": "npx turbo start-prod",
    "supabase": "cd apps/web && npx supabase",
    "supabase:db:diff": "npm run supabase -- db diff",
    "supabase:db:pull": "npm run supabase -- db pull && npm run supabase:gen:types",
    "supabase:db:push": "npm run supabase -- db push && npm run supabase:gen:types",
    "supabase:db:reset": "npm run supabase -- db reset && npm run supabase:gen:types",
    "supabase:db:sync-to-local": "npm run supabase migration up --local",
    "supabase:gen:types": "cd apps/web && npm run gen-types",
    "supabase:migrate": "sh ./scripts/create-supabase-migration.sh",
    "supabase:mock-user:init": "npm run web:run-script -- ./scripts/init/create-mock-supabase-user.ts",
    "supabase:prod:reset": "bash installer/supabase-docker/reset.sh",
    "supabase:prod:start": "bash installer/start-supabase.sh",
    "supabase:prod:stop": "bash installer/stop-supabase.sh",
    "supabase:restart": "npm run supabase -- stop && npm run supabase -- start && npm run supabase:gen:types",
    "supabase:status": "npm run supabase -- status",
    "supabase:storage:init": "npm run web:run-script -- ./scripts/init/register-local-storage-buckets.ts",
    "vercel:build-web": "npm run envs:pull && vercel pull --yes --token eFMlbwRscfzINIAZiPEXTeHr --environment=production && vercel build --token eFMlbwRscfzINIAZiPEXTeHr --prod",
    "web:run-script": "cd apps/web && env TS_NODE_PROJECT=./tsconfig.json npx ts-node -r tsconfig-paths/register"
  },
