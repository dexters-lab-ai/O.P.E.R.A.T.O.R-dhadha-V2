{
  "name": "@aident/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "db:up": "npx supabase migration up && npm run gen-types",
    "dev": "npm run db:up && next dev",
    "next-dev": "next dev",
    "build": "next build",
    "db:fetch": "npx supabase db push --dry-run",
    "db:pull": "npx supabase db pull; npm run gen-types",
    "db:reset": "npx supabase db reset && npm run db:pull",
    "db:revert": "npx supabase migration repair --status reverted",
    "docker:start": "npm run build",
    "docker:stop": "docker rm -f open-cuak-web",
    "exec": "TS_NODE_PROJECT=./tsconfig.json ts-node -r tsconfig-paths/register",
    "format": "npx prettier . --write",
    "format-check": "npx prettier . --check",
    "function-serve": "npx supabase functions serve --env-file ./supabase/functions/.env.functions.local",
    "function-test": "deno run --allow-all",
    "gen-types": "npx supabase gen types typescript --local > ../../packages/shared/src/schema.gen.ts",
    "repl": "npx ts-node --esm --interactive --compilerOptions '{\"target\": \"esnext\"}' --pretty",
    "start": "npx env-cmd -f .env.production next start -p 11970",
    "start-prod": "npx env-cmd -f .env.production next dev -p 11970",
    "supabase": "npx supabase",
    "test-deno": "deno test --allow-all"
  },
  "dependencies": {
    "@aident/shared": "workspace:*",
    "next": "15.2.4",
    "tailwindcss": "3.4.17",
    "@langchain/core": "0.3.43",
    "common-tags": "1.8.2",
    "@ai-sdk/amazon-bedrock": "2.2.3",
    "@ai-sdk/anthropic": "1.2.3",
    "@ai-sdk/azure": "1.3.5",
    "@ai-sdk/google": "1.2.4",
    "@ai-sdk/google-vertex": "2.2.6",
    "@aws-sdk/client-cloudfront": "3.777.0",
    "@aws-sdk/client-sso-oidc": "3.777.0",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.0",
    "@headlessui/react": "2.2.0",
    "@heroicons/react": "2.2.0",
    "@microlink/react-json-view": "1.26.1",
    "@mui/material": "5.17.1",
    "@opentelemetry/api-logs": "0.57.2",
    "@opentelemetry/instrumentation": "0.57.2",
    "@opentelemetry/sdk-logs": "0.57.2",
    "@supabase/auth-helpers-nextjs": "0.10.0",
    "@supabase/auth-js": "2.69.1",
